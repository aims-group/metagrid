{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.2.1",
    "@testing-library/jest-dom": "5.10.1",
    "@testing-library/react": "10.2.1",
    "@testing-library/user-event": "11.4.2",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "antd": "4.3.4",
    "axios": "0.19.2",
    "cors-anywhere": "0.4.3",
    "dotenv": "8.2.0",
    "lint-staged": "10.2.10",
    "prop-types": "15.7.2",
    "query-string": "6.13.1",
    "react": "16.13.1",
    "react-async": "10.0.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "3.9.5",
    "uuid": "8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "CI=1 react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "CI=1 react-scripts test --coverage --env=jest-environment-jsdom-sixteen",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "prettier": "2.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!src/index.tsx",
      "!**/serviceWorker.js",
      "!**/proxy.js",
      "!**/react-app-env.d.ts",
      "!**/custom_types/**",
      "!**/axios/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "./src/App.tsx": {
        "lines": 100
      }
    }
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx|json)": [
      "eslint --fix"
    ],
    "src/**/*.+(js|jsx|ts|tsx|css|sass|less|json)": [
      "prettier --write"
    ]
  }
}
